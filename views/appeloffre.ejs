    <!-- JqueryUI -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
    <link rel="stylesheet" type="text/css" href="css/jquery-ui.css">
    <!-- Bootstrap -->
    <link rel="stylesheet" type="text/css" href="./css/boostrap/bootstrap.min.css">
    <!-- Awesome font icons -->
    <link rel="stylesheet" type="text/css" href="./css/font-awesome.min.css">
    <link rel='stylesheet' href='https://use.fontawesome.com/releases/v5.7.0/css/all.css' integrity='sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ' crossorigin='anonymous'>
    <!-- Magnific popup-->
    <link rel="stylesheet" type="text/css" href="./css/magnific-popup/magnific-popup.css" media="screen" />
    <!-- Owlcoursel -->
    <link rel="stylesheet" type="text/css" href="./css/owl-coursel/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="./css/owl-coursel/owl.theme.css">
    <!-- Main style -->
    <link rel="stylesheet" type="text/css" href="./css/style.css">
    <!-- Padding / Margin -->
    <link rel="stylesheet" type="text/css" href="css/padd-mr.css">
    <!-- light version-->
    <link id="vers" rel="stylesheet" type="text/css" href="css/light-version.css">

<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
</head>
<body class="bg-3">
<!-- Preloader -->
<div class="preloader">
<p></p>
</div>
<div id="wrap" class="color1-inher">
    <% include partials/nav.ejs %>
    <!-- Main content -->
    <div id="wrap-body" class="p-t-lg-30">
        <div class="container">
            <div class="wrap-body-inner">
                <!-- Breadcrumb-->
                <div class="hidden-xs">
                    <div class="row">
                        <div class="col-lg-6">
                            <ul class="ht-breadcrumb pull-left">
                                <li class="home-act"><a href="#"><i class="fa fa-home"></i></a></li>
                                <li class="active">Appel d'Offre</li>
                            </ul>
                        </div>
                    </div>
                </div>
                <!-- Contact -->
                <form method="post" action="/appoffre">
                <div class="form-group col-md-6">
                    <div class="form-group input-group margin-bottom-sm">
                        <span class="input-group-addon"><i class="far fa-hand-point-right"></i></span>
                        <textarea cols="5" rows="5"class="form-control" name="value"></textarea>
                    </div>
                </div>
                <div class="form-group col-md-12">
                    <button type="submit" class="ht-btn ht-btn-default">vérification</button>
                </div>
                </form>
       
    
    <% if (taille > 0) {%>

        <div class="table-wrapper"></div>
        <table class="table table-hovered">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Image</th>
                    <th scope="col">Nom Et Prénom</th>
                    <th scope="col">Spécialité</th>
                    <th scope="col">Experience/An</th>
                    <th scope="col">Utilisateur</th>
                    <th scope="col">Nombre</th>
                    <th scope="col">File</th>
                    <th scope="col">Action</th>
                </tr>
            </thead>
            <tbody>
                   <!-- <div class="bs-example">
                            <input type="text" name="typeahead" class="typeahead tt-query" autocomplete="off" spellcheck="false" placeholder="">
                             </div>-->
                           <!--  <div class="bs-example1"> 
                            <form action="searchy" method="GET">      
                             <input type="text" name="typeahead1" id="typeahead1" class="typeahead1 tt-query" autocomplete="off" spellcheck="false" placeholder="">
                             <br><br> &nbsp;<input type="submit" value="Search" id="button"/>
                           </form>
                            </div>-->
                    <% for(var i=0; i<(taille); i++){ %>
                    <tr>
                       
                        <th scope="row"><%= players[i].id %></th>
                        <td><img src="/images/<%= players[i].image %>" style="border-radius: 50% !important; width:40px; height:40px" class="rounded-circle player-img" alt=""></td>
                        <td><a href="/detailss/<%= players[i].id %>"><%= players[i].lastname %> <%= players[i].firstname %></a></td>
                        <td><%= players[i].position %></td>
                        <td><%= players[i].experience %></td>
                        <td><%= user[0].username %></td>
                        <td><%= players[i].number %></td>
                        <td><a href="/file/<%= players[i].fileWord %>" target="_blank" rel="noopener" class="rounded-circle player-img" alt=""><img style="width:40px; height: 40px" src="/images/word.png"/></a>
                        <a href="/file/<%= players[i].filePDF %>" target="_blank" rel="noopener" class="rounded-circle player-img" alt=""><img style="width:40px; height: 40px" src="/images/pdf.png"/></a></td>
                        <td>
                            <a href="/edit/<%= players[i].id %>" class="btn btn-sm btn-success">Edit</a>
                            <a href="/delete/<%= players[i].id %>" class="btn btn-sm btn-danger">Delete</a>
                        </td>
                        <!--target="_blank" rel="noopener"-->
                    </tr>
                <% } }%>
            </tbody>
        </table>
    </div>
</div>
</div>
</div>
    <% include partials/footer.ejs %>
</div></div>
</div>
<!-- jQuery -->
<script src="js/jquery/jquery-2.2.4.min.js"></script>
